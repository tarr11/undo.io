<ul class="breadcrumb">
  <li><%= link_to "Services", :controller => "oauth_consumers"%></li>
  <span class="divider">/</span>
  <li><%=params[:id].humanize%></li>
</ul>
<h1><%=params[:id].humanize%></h1>
<div>You are connected to Dropbox</div>
<%= form_tag oauth_consumer_path(params[:id]),:method=>:delete do %>
  <%=submit_tag "Disconnect" %>
  or
  <%=submit_tag "Reconnect" %>
  if you experienced a problem.
<% end %>